  "description": "MCP server for Azure Developer CLI template operations",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "mcp-azd-template": "dist/cli.js"
  },
  "files": [
    "dist"
  ],  
